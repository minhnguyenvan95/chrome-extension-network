{
  "name": "Chrome Extension NetWork Client",
  "description": "Chrome Extension NetWork Client",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
